#include "Register.h"

REGISTER* RegInit(void){
	REGISTER* list = (REGISTER*)calloc(1,sizeof(REGISTER*));
	reg = NULL;
	return reg;
}

REGISTER* CreateNewRegister(REGISTER* reg){
	REGISTER* new = malloc(sizeof(REGISTER*));
	new->id = 0;
	new->next = NULL;
	if(reg == NULL){
		reg = new;
		return reg;
	}
	else{
		new->id = red->id + 1;
		new->next = reg;
		return new;
	}	
}

void ShowRegisters(REGISTER* reg);{

	while(reg != NULL){
		printf("Reg%d \n", reg->id);
		reg = reg->next;
	}
}

